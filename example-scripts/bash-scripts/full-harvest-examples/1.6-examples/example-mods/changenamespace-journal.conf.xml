<Task>
	<!--
	inputModel - relative path to the configuration file for the Jena model in which we are to
	             perform the find/replace.
	-->
	<Param name="inputModel">harvested-data.model.xml</Param>
	
	<!--
	vivoModel - relative path to the configuration file for the Jena model to use as a reference to
	            ensure renamed entities have unique names.
	-->
	<Param name="vivoModel">vivo.model.xml</Param>
	
	<!--
	oldNamespace - namespace to search for.
	-->
	<Param name="oldNamespace">http://vivoweb.org/harvest/mods/journal/</Param>
	
	<!--
	newNamespace - namespace with which to replace the old namespace.
	-->
	<Param name="newNamespace">http://localhost/individual/</Param>
</Task>
